@using MvcKutuphane.Models.Entity
@model List<TBLHAREKET>
@{
    ViewBag.Title = "UyeKitapGecmis";
    Layout = "~/Views/Shared/Layout.cshtml";
}
<br />
<h2>@ViewBag.Uye</h2>
<br />
<table class=" table table-danger">
    <tr>
        <th>Kitap Adı</th>
        <th>Personel</th>
        <th>Alış Tarihi</th>
        <th>İade Tarihi</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.TBLKITAP.AD</td>
            <td>@item.TBLPERSONEL.PERSONEL</td>
            <td>@Convert.ToDateTime(item.ALISTARIH).ToString("dd.MM.yyyy")</td>
            <td>@Convert.ToDateTime(item.UYEGETİRTARİH).ToString("dd.MM.yyyy")</td>
        </tr>

    }
</table>
